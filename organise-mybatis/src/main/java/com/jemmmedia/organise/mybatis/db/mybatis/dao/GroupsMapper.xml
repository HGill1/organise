  </select>
  
  <select id="selectEmailsForRemiders" resultType="string">
  	select GROUP_CONCAT(u.email) from groups g 
	inner join users u on g.id = u.group_id  
	inner join roles_users ru on ru.user_id = u.id 
	where g.package_id = 1 
	and  (current_date = DATE_ADD(Date(g.created_at), INTERVAL 9 DAY)  or current_date = DATE_ADD(Date(g.created_at), INTERVAL 1 DAY))
	and ru.role_id = 1  
  </select>
  
