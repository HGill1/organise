  <select id="selectAllSources" parameterType="java.lang.Long" resultType="com.jemmmedia.organise.service.impl.bean.FeedSourceExtBean">
  	select fs.*,GROUP_CONCAT(' ',u.first_name,' ',last_name) users
	from feed_sources fs left join feed_sources_users fsu on fs.id = fsu.feed_source_id 
	left join users u on fsu.user_id=u.id
	where fs.group_id = #{groupId,jdbcType=BIGINT} 
	group By fs.id  
  </select> 
  <select id="selectFeedSources" resultType="com.jemmmedia.organise.service.impl.bean.FeedSourceExtBean">
     select fs.*, u.id as rssUserId, fs.source_type as sourceType from feed_sources fs 
     inner join users u on fs.id = u.user_type and parent_id is null;
  </select>
  
  <select id="selectUsersFeedSources" parameterType="String" resultMap="FeedSourceMap">
     select fs.id, fs.name,fs.colour,fs.source_type,fs1.id as subtopicId,fs1.name as subtopicName,fs1.colour as subtopicColor 
     from feed_sources fs 
     inner join feed_sources_users fsu on fs.id= fsu.feed_source_id
     left join feed_sources fs1 on fs1.parent_id = fs.id  
     where fsu.user_id=#{id,jdbcType=INTEGER} and fs.parent_id is null;
  </select> 
  
  <select id="selectFeedSourceForEdit" parameterType="String" resultType="com.jemmmedia.organise.service.impl.bean.FeedSourceExtBean">
    select fs.id,fs.name,fs.url, fs.token,fs.colour,fs.source_type as sourceType,fs2.parent_id as parentId,fs.group_id as groupId, GROUP_CONCAT(distinct u.id) as usersId
 	from feed_sources fs 
	left join feed_sources_users fsu on fs.id= fsu.feed_source_id 
	left join users u on fsu.user_id= u.id 
	left join feed_sources fs2 on fs.id=fs2.parent_id
	where fs.id = #{id,jdbcType=INTEGER} group by fs.id
  </select> 
  
   <select id="selectFollowCat" parameterType="String" resultType="com.jemmmedia.organise.service.impl.db.mybatis.model.FeedSource">
     select id, name, colour from feed_sources where id in(select feed_source_id from feed_entries where follower = #{followerIds});
  </select>
  
  <select id="selectSubTopics" parameterType="String" resultType="com.jemmmedia.organise.service.impl.bean.FeedSourceExtBean">
