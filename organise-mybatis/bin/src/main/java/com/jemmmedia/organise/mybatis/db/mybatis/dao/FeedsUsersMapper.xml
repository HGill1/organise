		left join dept_users du on du.user_id = fe.assign_to_dept left join dept d on d.id = fe.assign_to_dept 
		where  fe.parent_feed = true AND status != 4 AND fe.feed_source_id in(select id from feed_sources fs 
	    inner join feed_sources_users fsu on fs.id= fsu.feed_source_id where fsu.user_id=#{userId,jdbcType=INTEGER}) 
    
    union
    
	    select fe.id, fe.user_id,fe.name,fs.name as feedSourceName,fe.feed_source_id as feedSourceId,fs.colour,fe.deadline, fe.follower, fe.feed_type as feedType,
